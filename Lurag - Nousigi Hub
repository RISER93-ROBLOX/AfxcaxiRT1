if isfolder("Nousigi Hub") then
    if isfile("Nousigi Hub/savedkey") then
        local cnt = readfile("Nousigi Hub/savedkey")
        if cnt ~= "PRRFHDLzyRNbiVsfJPuJYFtxiSrNcEMb" then 
            writefile("Nousigi Hub/savedkey", "PRRFHDLzyRNbiVsfJPuJYFtxiSrNcEMb")
        end
    else
        writefile("Nousigi Hub/savedkey", "PRRFHDLzyRNbiVsfJPuJYFtxiSrNcEMb") 
    end
else
    makefolder("Nousigi Hub") 
    writefile("Nousigi Hub/savedkey", "PRRFHDLzyRNbiVsfJPuJYFtxiSrNcEMb") 
end
